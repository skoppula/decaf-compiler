callout printf;
void main ( ) {
  int quot;
  quot = 8%2;
  
  printf ( "Should equal 0, is %ld\n", quot );

  quot = 5%3;

  printf ( "Should equal 2, is %ld\n", quot );

  quot = 5%-3;

  printf ( "Should equal 2, is %ld\n", quot );

  quot = -5%3;

  printf ( "Should equal -2, is %ld\n", quot );
}
